{
  "id": "sheet-only",
  "title": "Sheet Only",
  "description": "Provides a fullscreen character sheet to use on tablets",
  "version": "2.0.2",
  "compatibility": {
    "minimum": "13",
    "verified": "13.346"
  },
  "authors": [
    {
      "name": "Syrious",
      "discord": "syrious_",
      "flags": {
        "patreon": "SyriousWorkshop",
        "github": "Syrious",
        "ko-fi": "syrious"
      }
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json",
      "flags": {}
    },
    {
      "lang": "de",
      "name": "Deutsch",
      "path": "lang/de.json",
      "flags": {}
    }
  ],
  "esmodules": [
    "index.js"
  ],
  "styles": [
    "style.css"
  ],
  "flags": {
    "hotReload": {
      "extensions": [
        "json"
      ],
      "paths": [
        "lang"
      ]
    }
  },
  "socket": true,
  "bugs": "https://discord.gg/VMqndcyUGS",
  "url": "${module-url}",
  "manifest": "${module-endpoint}",
  "download": "${download-endpoint}"
}